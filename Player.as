package  {	import flash.display.Sprite;		public class Player extends Sprite{				public var redDude:Dude = new Dude();		public var blueDude:Dude = new Dude();		public var yellowDude:Dude = new Dude();				public var shields:int = 20;		public var maxShields:int = 20;				public function Player() {			// constructor code						makeDudes();		}				public function makeDudes():void{			redDude.color = "red";			addChild(redDude);			blueDude.color = "blue";			blueDude.y = 40;			addChild(blueDude);			yellowDude.color = "yellow";			yellowDude.y = 80;			addChild(yellowDude);						redDude.graphics.beginFill(0xFF0000, 1);			redDude.graphics.drawRect(0, 0, 20, 20);						blueDude.graphics.beginFill(0x0000FF, 1);			blueDude.graphics.drawRect(0, 0, 20, 20);						yellowDude.graphics.beginFill(0xFFFF00, 1);			yellowDude.graphics.drawRect(0, 0, 20, 20);		}	}	}