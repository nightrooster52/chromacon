package {	//the playerEffects object will be a container for the	//various combinations of color effects	public class PlayerEffects {		public var main:Prototype;		public var redEffect:Effect;		public var yellowEffect:Effect;		public var blueEffect:Effect;		public var greenEffect:Effect;		public var orangeEffect:Effect;		public var purpleEffect:Effect;		//white effects will in general be something special		public var whiteEffect:WhiteEffect;		public function PlayerEffects(main:Prototype) {			// constructor code			this.main = main;			//default effects			defaultEffects();		}		public function defaultEffects() {			redEffect = new Effect(main);			yellowEffect = new Effect(main);			blueEffect = new Effect(main);			greenEffect = new Effect(main);			orangeEffect = new Effect(main);			purpleEffect = new Effect(main);						whiteEffect = new WhiteEffect(main);			redEffect.healthDamage = 5;			yellowEffect.armorDamage = 3;			blueEffect.shielding = 2;		}		public function activate(color:String, blocks:int) {			if (color == "red") {				redEffect.activate(blocks);				main.player.redForward();				main.player.blueBack();				main.player.yellowBack();							} else if (color == "yellow") {				yellowEffect.activate(blocks);				main.player.redBack();				main.player.yellowForward();				main.player.blueBack();							} else if (color == "blue") {				blueEffect.activate(blocks);				main.player.redBack();				main.player.blueForward();				main.player.yellowBack();							} else if (color == "green") {				greenEffect.activate(blocks);				main.player.redBack();				main.player.blueForward();				main.player.yellowForward();			} else if (color == "orange") {				orangeEffect.activate(blocks);				main.player.redForward();				main.player.blueBack();				main.player.yellowForward();			} else if (color == "purple") {				purpleEffect.activate(blocks);				main.player.redForward();				main.player.blueForward();				main.player.yellowBack();			} else if (color == "white") {				whiteEffect.activate(blocks);				main.player.redForward();				main.player.blueForward();				main.player.yellowForward();			}		}	}}