package  {		//the playerEffects object will be a container for the	//various combinations of color effects	public class PlayerEffects {				public var main:Prototype;				public var redEffect:Effect;		public var yellowEffect:Effect;		public var blueEffect:Effect;		public var greenEffect:Effect;		public var orangeEffect:Effect;		public var purpleEffect:Effect;				 //white effects will in general be something special		public var whiteEffect:WhiteEffect;				public function PlayerEffects(main:Prototype) {			// constructor code			this.main = main;						//default effects			defaultEffects();		}				public function defaultEffects(){			redEffect = new Effect(main);			yellowEffect = new Effect(main);			blueEffect = new Effect(main);			greenEffect = new Effect(main);			orangeEffect = new Effect(main);			purpleEffect = new Effect(main);						redEffect.healthDamage = 5;			yellowEffect.armorDamage = 3;			blueEffect.shielding = 2;		}	}	}