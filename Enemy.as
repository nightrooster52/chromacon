package  {			import flash.display.Sprite;		public class Enemy extends Sprite{				public var health:int = 100;		public var maxHealth:int = 100;		public var armor:int = 20;		public var maxArmor:int = 20;				public function Enemy() {			// constructor code			graphics.beginFill(0x000000, 1);			graphics.drawRect(0, 0, 40, 40);		}		public function takeHealthDamage(damage:int):void{			damage -= armor;			if (damage < 0){				damage = 0;			}			health -= damage;			trace("enemy took ", damage, " damage");			trace("enemy HEALTH: ",health);		}		public function takeArmorDamage(damage:int):void{			armor -= damage;			if (armor < 0){				armor = 0;			}			trace("enemy took ", damage, " armor damage");			trace("enemy ARMOR: ",armor);					}		public function heal(amount:int):void{			health += amount;			if (health > maxHealth){				health = maxHealth;			}			trace("enemy healed ", amount);			trace("enemy HEALTH", health);		}		public function buildArmor(amount:int):void{			armor += amount;			if (armor > maxArmor){				armor = maxArmor;			}			trace("enemy built ", amount, "armor");			trace("enemy Armor ", armor);		}	}	}