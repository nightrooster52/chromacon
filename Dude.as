package  {	import flash.display.Sprite;		public class Dude extends Sprite{		public var health:int = 100;		public var maxHealth:int = 100;		public var position:String;		public var color:String;		public function Dude() {			// constructor code			health = 100;		}				public function moveForward():void{			position = "forward";			//move x to front		}		public function moveBack():void{			position = "back";			//move x to back		}	}	}